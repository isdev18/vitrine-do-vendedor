{
  "version": 2,
  "name": "vitrine-do-vendedor",
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/vitrine",
      "dest": "/vitrine.html"
    },
    {
      "src": "/painel",
      "dest": "/painel.html"
    },
    {
      "src": "/login",
      "dest": "/login.html"
    },
    {
      "src": "/cadastro",
      "dest": "/cadastro.html"
    },
    {
      "src": "/planos",
      "dest": "/planos.html"
    },
    {
      "src": "/admin",
      "dest": "/admin.html"
    },
    {
      "src": "/contato",
      "dest": "/contato.html"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
