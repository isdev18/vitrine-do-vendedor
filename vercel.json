{
  "rewrites": [
    {
      "source": "/vitrine",
      "destination": "/vitrine.html"
    },
    {
      "source": "/painel",
      "destination": "/painel.html"
    },
    {
      "source": "/login",
      "destination": "/login.html"
    },
    {
      "source": "/cadastro",
      "destination": "/cadastro.html"
    },
    {
      "source": "/planos",
      "destination": "/planos.html"
    },
    {
      "source": "/admin",
      "destination": "/admin.html"
    },
    {
      "source": "/contato",
      "destination": "/contato.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
